{
    "name": "anthonyedmonds/laravel-find",
    "description": "Find models within your Laravel system with minimal overhead",
    "keywords": ["laravel", "find", "search", "query builder"],
    "type": "library",
    "homepage": "https://github.com/AnthonyEdmonds/laravel-find",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthony Edmonds",
            "email": "contact@anthonyedmonds.me",
            "homepage": "https://anthonyedmonds.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^11"
    },
    "require-dev": {
        "laravel/pint": "^1",
        "orchestra/testbench": "^9",
        "phpunit/phpunit": "^11"
    },
    "autoload": {
        "psr-4": {
            "AnthonyEdmonds\\LaravelFind\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AnthonyEdmonds\\LaravelFind\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AnthonyEdmonds\\LaravelFind\\FindServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
